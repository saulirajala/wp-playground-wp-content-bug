            "max": "3rem",
            "min": "2.5rem"
          },
          "name": "xx-Large",
          "size": "3rem",
          "slug": "xx-large"
        },
        {
          "fluid": {
            "max": "3.5rem",
            "min": "3rem"
          },
          "name": "xxx-Large",
          "size": "3.5rem",
          "slug": "xxx-large"
        },
        {
