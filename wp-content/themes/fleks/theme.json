{
  "$schema": "https://schemas.wp.org/wp/6.4/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "background": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultDuotone": true,
      "defaultGradients": true,
      "defaultPalette": true,
      "duotone": [],
      "gradients": [
        {
          "gradient": "linear-gradient(135deg,rgb(20,30,48) 0%,var(--wp--preset--color--primary) 100%)",
          "name": "Primary to Black",
          "slug": "primary-to-black"
        }
      ],
      "palette": [
        {
          "color": "#FFFEFD",
          "name": "Base",
          "slug": "base"
        },
        {
          "color": "#18191a",
          "name": "Contrast",
          "slug": "contrast"
        },
        {
          "color": "#009E66",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#00B380",
          "name": "Hover",
          "slug": "hover"
        },
        {
          "color": "#f0f9ee",
          "name": "Soft",
          "slug": "soft"
        }
      ],
      "text": true
    },
    "custom": [],
    "layout": {
      "contentSize": "740px",
      "wideSize": "1280px"
    },
    "lightbox": {
      "allowEditing": true,
      "enabled": true
    },
    "shadow": {
      "presets": [
        {
          "name": "Small",
          "shadow": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
          "slug": "sm"
        },
        {
          "name": "Medium",
          "shadow": "0 1px 3px 0 rgb(0 0 0 / 0.1), 0 1px 2px -1px rgb(0 0 0 / 0.1)",
          "slug": "md"
        },
        {
          "name": "Large",
          "shadow": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
          "slug": "lg"
        },
        {
          "name": "XL",
          "shadow": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)",
          "slug": "xl"
        },
        {
          "name": "2XL",
          "shadow": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)",
          "slug": "2-xl"
        },
        {
          "name": "Inner",
          "shadow": "inset 0 2px 4px 0 rgb(0 0 0 / 0.05)",
          "slug": "inner"
        }
      ]
    },
    "spacing": {
      "spacingScale": {
        "steps": 0
      },
      "spacingSizes": [
        {
          "name": "1",
          "size": "0.313rem",
          "slug": "10"
        },
        {
          "name": "2",
          "size": "0.75rem",
          "slug": "20"
        },
        {
          "name": "3",
          "size": "clamp( 1.34rem, 1.5vw + 0.89rem,  1.86rem )",
          "slug": "30"
        },
        {
          "name": "4",
          "size": "clamp(2rem, 1.8846rem + 0.5128vw, 2.5rem)",
          "slug": "40"
        },
        {
          "name": "5",
          "size": "clamp(3rem, 2.8269rem + 0.7692vw, 3.75rem)",
          "slug": "50"
        },
        {
          "name": "6",
          "size": "clamp(4rem, 3.7692rem + 1.0256vw, 5rem)",
          "slug": "60"
        },
        {
          "name": "7",
          "size": "clamp(6rem, 5.6538rem + 1.5385vw, 7.5rem)",
          "slug": "70"
        }
      ],
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "customFontSize": true,
      "dropCap": true,
      "fluid": true,
      "fontFamilies": [
        {
          "fontFace": [
            {
              "fontFamily": "Sen",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": [
                "file:./assets/fonts/sen_normal_400.ttf"
              ]
            },
            {
              "fontFamily": "Sen",
              "fontStyle": "normal",
              "fontWeight": "600",
              "src": [
                "file:./assets/fonts/sen_normal_600.ttf"
              ]
            }
          ],
          "fontFamily": "Sen",
          "slug": "font-sen"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Now",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": [
                "file:./assets/fonts/now_normal_700.ttf"
              ]
            }
          ],
          "fontFamily": "Now",
          "slug": "font-now"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Poppins",
              "fontStyle": "normal",
